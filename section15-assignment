CREATE OR REPLACE TABLE EXERCISE_DB.PUBLIC.SUPPLIER
AS
SELECT * FROM "SNOWFLAKE_SAMPLE_DATA"."TPCH_SF1"."SUPPLIER";

CREATE TABLE EXERCISE_DB.PUBLIC.SUPPLIER_CLONE 
CLONE EXERCISE_DB.PUBLIC.SUPPLIER;

UPDATE SUPPLIER_CLONE SET S_PHONE='###';


CREATE TABLE EXERCISE_DB.PUBLIC.SUPPLIER_CLONE_1 
CLONE EXERCISE_DB.PUBLIC.SUPPLIER_CLONE BEFORE (statement=>'01a59fdd-3200-7d44-0001-313a0001e082');


DROP TABLE EXERCISE_DB.PUBLIC.SUPPLIER;

SELECT * FROM EXERCISE_DB.PUBLIC.SUPPLIER_CLONE;
